    <altmember cref="M:Microsoft.VisualBasic.FileSystem.GetAttr(System.String)" />
    <altmember cref="M:Microsoft.VisualBasic.FileSystem.SetAttr(System.String,Microsoft.VisualBasic.FileAttribute)" />
