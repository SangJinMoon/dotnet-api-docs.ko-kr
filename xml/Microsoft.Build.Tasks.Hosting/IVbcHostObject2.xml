        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.Hosting.IVbcHostObject2.SetOptionInfer(System.Boolean)">
          <source>Returns <ph id="ph1">&lt;see langword="true" /&gt;</ph> if the method was successful; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
