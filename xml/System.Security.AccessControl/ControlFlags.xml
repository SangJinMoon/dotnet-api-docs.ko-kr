        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Security.AccessControl.ControlFlags.DiscretionaryAclAutoInherited">
          <source>Specifies that the Discretionary Access Control List (DACL) has been automatically inherited from the parent.</source>
          
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Security.AccessControl.ControlFlags.DiscretionaryAclAutoInherited">
          <source>Set by resource managers only.</source>
          
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Security.AccessControl.ControlFlags.DiscretionaryAclAutoInheritRequired">
          <source>Ignored.</source>
          
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Security.AccessControl.ControlFlags.DiscretionaryAclDefaulted">
          <source>Specifies that the DACL was obtained by a defaulting mechanism.</source>
          
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Security.AccessControl.ControlFlags.DiscretionaryAclDefaulted">
