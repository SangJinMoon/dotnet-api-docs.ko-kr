      <MemberSignature Language="F#" Value="member this.SetFontSubsettingCountPolicy : int -&gt; unit" Usage="xpsSerializationManager.SetFontSubsettingCountPolicy countPolicy" />
      <MemberType>Method</MemberType>
      <AssemblyInfo>
        <AssemblyName>ReachFramework</AssemblyName>
        <AssemblyVersion>3.0.0.0</AssemblyVersion>
        <AssemblyVersion>4.0.0.0</AssemblyVersion>
