        <trans-unit id="117" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.DirectoryServices.Protocols.DirectoryControl.ServerSide">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the control is a server-side control.</source>
          <target state="translated">컨트롤이 서버 쪽 컨트롤이면 <ph id="ph1">&lt;see langword="true" /&gt;</ph>입니다.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.DirectoryServices.Protocols.DirectoryControl.ServerSide">
