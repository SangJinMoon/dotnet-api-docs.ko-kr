        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Description.ServiceMetadataContractBehavior.System#ServiceModel#Description#IContractBehavior#ApplyClientBehavior(System.ServiceModel.Description.ContractDescription,System.ServiceModel.Description.ServiceEndpoint,System.ServiceModel.Dispatcher.ClientRuntime)">
