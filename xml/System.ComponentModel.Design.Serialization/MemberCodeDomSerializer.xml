        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.ComponentModel.Design.Serialization.MemberCodeDomSerializer.Serialize(System.ComponentModel.Design.Serialization.IDesignerSerializationManager,System.Object,System.ComponentModel.MemberDescriptor,System.CodeDom.CodeStatementCollection)">
          <source>The descriptor of the member to serialize.</source>
